#!/bin/sh -vx

if [ ! -d wo/build/LegislatureWO.woa ]; then
    echo ""
    echo "Please re-build the WO application and run this again."
    echo ""
    exit 1
fi

cd wo/build/LegislatureWO.woa

SLF=/System/Library/Frameworks
fRJ=framework/Resources/Java
home=/Users/ray
roots=/Users/ray/Roots

$SLF/JavaVM.framework/Home/bin/java -DWOUserDirectory=$home/Projects/LegislatureSQL/wo/build/LegislatureWO.woa -DNSProjectSearchPath='(..,../..)' -DNSProjectBundleEnabled=true -Dfile.encoding=UTF-8 -classpath $home/Projects/LegislatureSQL/wo/build/LegislatureWO.woa/Contents/Resources/Java:$roots/ERExtensions.$fRJ/ERExtensions.jar:$roots/ERJars.$fRJ/ERJars.jar:$roots/ERJars.$fRJ/ERFoundation.jar:$roots/ERJars.$fRJ/ERWebObjects.jar:$roots/ERJars.$fRJ/commons-cli-1.2.jar:$roots/ERJars.$fRJ/commons-codec-1.6.jar:$roots/ERJars.$fRJ/commons-io-2.2.jar:$roots/ERJars.$fRJ/commons-lang-2.6.jar:$roots/ERJars.$fRJ/commons-logging-1.1.1.jar:$roots/ERJars.$fRJ/erxservletadaptor.jar:$roots/ERJars.$fRJ/fluent-hc-4.2.5.jar:$roots/ERJars.$fRJ/httpclient-4.2.5.jar:$roots/ERJars.$fRJ/httpclient-cache-4.2.5.jar:$roots/ERJars.$fRJ/httpcore-4.2.4.jar:$roots/ERJars.$fRJ/httpmime-4.2.5.jar:$roots/ERJars.$fRJ/icu4j-3_8_1.jar:$roots/ERJars.$fRJ/joda-time-2.2.jar:$roots/ERJars.$fRJ/junit-4.10.jar:$roots/ERJars.$fRJ/log4j-1.2.17.jar:$roots/ERJars.$fRJ/ognl-2.6.7.jar:$roots/ERJars.$fRJ/slf4j-api-1.7.5.jar:$roots/ERJars.$fRJ/slf4j-log4j12-1.7.5.jar:$roots/ERPrototypes.$fRJ/ERPrototypes.jar:$roots/JavaWOExtensions.$fRJ/JavaWOExtensions.jar:$roots/WOOgnl.$fRJ/WOOgnl.jar:$roots/Ajax.$fRJ/Ajax.jar:$roots/Ajax.$fRJ/jabsorb-1.2.2.jar:$SLF/JavaEOAccess.$fRJ/javaeoaccess.jar:$SLF/JavaEOAccess.framework/WebServerResources/Java/JavaEOAccess.jar:$SLF/JavaEOControl.$fRJ/javaeocontrol.jar:$SLF/JavaEOControl.framework/WebServerResources/Java/JavaEOControl.jar:$SLF/JavaFoundation.$fRJ/javafoundation.jar:$SLF/JavaFoundation.framework/WebServerResources/Java/JavaFoundation.jar:$SLF/JavaJDBCAdaptor.$fRJ/javajdbcadaptor.jar:$SLF/JavaJDBCAdaptor.framework/WebServerResources/Java/JavaJDBCAdaptor.jar:$SLF/JavaWebObjects.$fRJ/javawebobjects.jar:$SLF/JavaWebObjects.framework/WebServerResources/Java/JavaWebObjects.jar:$SLF/JavaXML.$fRJ/javaxml.jar:$SLF/JavaXML.framework/WebServerResources/Java/javaxml.jar:$home/Projects/LegislatureSQL/wo/Libraries/annotations.jar:$home/Projects/LegislatureSQL/wo/Libraries/jsr305.jar org.ganymede.leginfo.Application -WOPort 55554 -DWOUserDirectory=$home/Projects/LegislatureSQL/wo/build/LegislatureWO.woa -EOAdaptorDebugEnabled false -DNSProjectSearchPath '(..,../..)' -WOAcceptMalformedCookies false -WOAdaptorURL http://127.0.0.1/cgi-bin/WebObjects -WOAdditionalAdaptors '()' -WOAllowRapidTurnaround true -WOAllowsCacheControlHeader true -WOAllowsConcurrentRequestHandling false -WOAutoOpenClientApplication false -WOAutoOpenInBrowser false -WOApplicationBaseURL /WebObjects -WOApplicationClassName org.ganymede.leginfo.Application -WOCachingEnabled false -WOContextClassName WOContext -WODebuggingEnabled true -WOAdaptor WODefaultAdaptor -WODefaultUndoStackLimit 10 -WODirectConnectEnabled true -WODisplayExceptionPages true -WOHost 127.0.0.1 -WOIDE WOLips -NSOpenProjectIDE WOLips -WOFrameworksBaseURL /WebObjects/Frameworks -WOGenerateWSDL true -WOIncludeCommentsInResponse false -WOLifebeatDestinationPort 1085 -WOLifebeatEnabled true -WOLifebeatInterval 30 -WOListenQueueSize 128 -WOMaxHeaders 200 -WOMaxIOBufferSize 8196 -WOMaxSocketIdleTime 180000 -WOMissingResourceSearchEnabled true -WOMonitorEnabled false -WOSessionTimeOut 3600 -WOSessionStoreClassName WOServerSessionStore -WOSMTPHost smtp -WOSocketCacheSize 100 -WOSocketMonitorSleepTime 50 -WOWorkerThreadCount 8 -WOWorkerThreadCountMin 16 -WOWorkerThreadCountMax 256 -DNSProjectBundleEnabled true -DRunFixesAndQuit=true
